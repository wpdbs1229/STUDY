class Solution {
    public int solution(int i, int j, int k) {
        int answer = 0;
        for (int x = i; x < j + 1; x++){
            for (char c : String.valueOf(x).toCharArray() ){
                if( Character.getNumericValue(c) == k)
                    answer++;
            }
        }
        return answer;
    }
}
