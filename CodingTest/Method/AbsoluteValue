import java.util.Scanner;

public class AbsoluteValue {  //두 정수의 차를 절대값으로 반환해주는 메소드

	public int minusAbsoluteValue(int num1, int num2) {
		int result = 0;
		
		result = Math.abs(num1-num2); //절대값을 반환해주는 Math.abs 메소드
		
		return result;
	}
	
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		AbsoluteValue oper = new AbsoluteValue();
		Scanner scan = new Scanner(System.in);
		System.out.println("차의 절대값을 구할 두 정수를 입력해주세요");
		int num1 = scan.nextInt();
		int num2 = scan.nextInt();
		
		System.out.println(oper.minusAbsoluteValue(num1, num2));
	}

}
